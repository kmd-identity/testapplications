@model KMD.Identity.TestApplications.OpenID.MVCCore.Models.ApiCallResult<decimal[]>

@if (Model.HasError)
{
    <div class="alert alert-danger" role="alert">
        @Html.Raw(Model.Error)
    </div>
}
else
{
    <div class="alert alert-secondary" role="alert">
        @Html.Raw($"Payments done ({string.Join(", ", Model.Result.Select(r=>$"{r}$"))})")
    </div>
}
